{
    "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true,
          "completionSnippetsOn": false,
          "formattingProvider": "brittany"
        }
      }
    }
  },
  "coc.source.emoji.filetypes": ["markdown", "vimwiki"],
  "suggest.enablePreview":true
}
